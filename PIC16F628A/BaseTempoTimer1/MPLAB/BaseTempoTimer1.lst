                      00001 ;
                      00002 ;               Este programa tem a função de demonstrar como criar uma base de tempo a partir do timer1
                            . A base de tempo tem
                      00003 ;       a função de, a partir de um tempo mínimo de overflow, criar outras bases de tempo com o auxílio 
                            de um registrador
                      00004 ;       de uso geral (variável). Neste exemplo, será criada uma base de tempo de 1s.
                      00005 ;
                      00006 ;
                      00007 ;       *** Timer 1 ***
                      00008 ;       
                      00009 ;       Overflow = (65536 - <TMR1H::TMR1L>) * prescaler * ciclo de máquina
                      00010 ;               
                      00011 ;                                                                                       Overflow
                      00012 ;       (65536 - <TMR1H::TMR1L>) =      ---------------------------------
                      00013 ;                                                                  prescaler * ciclo de máquina 
                      00014 ;
                      00015 ;                                                                               Overflow
                      00016 ;       <TMR1H::TMR1L> = 65536 - ---------------------------------
                      00017 ;                                                               prescaler * ciclo de máquina
                      00018 ;
                      00019 ;                                                          200ms
                      00020 ;       <TMR1H::TMR1L> = 65536 - --------- = 15536
                      00021 ;                                                         4 * 1us
                      00022 ;
                      00023 ;       <TMR1H::TMR1L> = 15536d = 3CB0h
                      00024 ;
                      00025 ;       TMR1H = 3Ch
                      00026 ;       TMR1L = B0h
