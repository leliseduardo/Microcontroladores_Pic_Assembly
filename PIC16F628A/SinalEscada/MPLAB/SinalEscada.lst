                      00001 ;
                      00002 ;               O objetivo deste programa é gerar um sinal do tipo escada no pino RA2, a partir da tensã
                            o de referência de saída
                      00003 ;       neste pino. Para isso será implementado uma interrupção do timer1 a cada 500ms, que terá como fu
                            nção diminuir o valor
                      00004 ;       das flags VR, de forma que na saída RA2 será gerado um sinal do tipo escada.
                      00005 ;
                      00006 ;       *** Timer 1 ***
                      00007 ;       
                      00008 ;       Overflow = (65536 - <TMR1H::TMR1L>) * prescaler * ciclo de máquina
                      00009 ;               
                      00010 ;                                                                                       Overflow
                      00011 ;       (65536 - <TMR1H::TMR1L>) =      ---------------------------------
                      00012 ;                                                                  prescaler * ciclo de máquina 
                      00013 ;
                      00014 ;                                                                               Overflow
                      00015 ;       <TMR1H::TMR1L> = 65536 - ---------------------------------
                      00016 ;                                                               prescaler * ciclo de máquina
                      00017 ;
                      00018 ;                                                          500ms
                      00019 ;       <TMR1H::TMR1L> = 65536 - --------- = 3036
                      00020 ;                                                         8 * 1us
                      00021 ;
                      00022 ;       <TMR1H::TMR1L> = 3036d = 0BDCh
                      00023 ;
                      00024 ;       TMR1H = 0Bh
                      00025 ;       TMR1L = DCh     
