                      00001 ;
                      00002 ;               O objetivo deste programa é utilizar este código, feito na aula passada, denominada "Cor
                            recaoLatencia", e aplicar
                      00003 ;       num circuito com um buzzer, produzindo uma sirene de 1 tom.
                      00004 ;
                      00005 ;
                      00006 ;       *** Timer 1 ***
                      00007 ;       
                      00008 ;       Overflow = (65536 - <TMR1H::TMR1L>) * prescaler * ciclo de máquina
                      00009 ;               
                      00010 ;                                                                                       Overflow
                      00011 ;       (65536 - <TMR1H::TMR1L>) =      ---------------------------------
                      00012 ;                                                                  prescaler * ciclo de máquina 
                      00013 ;
                      00014 ;                                                                               Overflow
                      00015 ;       <TMR1H::TMR1L> = 65536 - ---------------------------------
                      00016 ;                                                               prescaler * ciclo de máquina
                      00017 ;
                      00018 ;                                                          500us
                      00019 ;       <TMR1H::TMR1L> = 65536 - --------- = 65036
                      00020 ;                                                         1 * 1us
                      00021 ;
                      00022 ;       <TMR1H::TMR1L> = 65036d = FE0Ch
                      00023 ;
                      00024 ;       TMR1H = FEh
                      00025 ;       TMR1L = 0Ch
                      00026 ;
                      00027 ;       Tempo de latência obtido pelo debug = 14us
                      00028 ;
                      00029 ;                                                         (500us - 14us)
                      00030 ;       <TMR1H::TMR1L> = 65536 - ---------------- = 65050 (Valor corrigido considerando a latência de in
                            terrupção)
                      00031 ;                                                                 1 * 1us
                      00032 ;
                      00033 ;       <TMR1H::TMR1L> = 65050d = FE1Ah
                      00034 ;
                      00035 ;       TMR1H = FEh
                      00036 ;       TMR1L = 1Ah     
                      00037 ;
                      00038 ;               No debug e na simulação o programa funcionou como esperado, com o tempo de Overflow exat
                            o, com o tempo de 
                      00039 ;       latência corrigido.
                      00040 ;
