                      00001 ;
                      00002 ;               Este programa tem o objetivo de demosntrar como corrigir o erro de latência da interrupç
                            ão do timer. Para fazer 
                      00003 ;       tal correção, é necessário, através do debug, descobrir qual é o tempo de latência da interrupçã
                            o e, a partir disso,
                      00004 ;       subtrair do contador do timer1 o tempo extra de latência. Isto é, já que a cada ciclo de máquina
                             o contador incrementa
                      00005 ;       1, basta descobrir o tempo de latência e subtrair esse tempo diretamente no contador. 
                      00006 ;
                      00007 ;       *** Timer 1 ***
                      00008 ;       
                      00009 ;       Overflow = (65536 - <TMR1H::TMR1L>) * prescaler * ciclo de máquina
                      00010 ;               
                      00011 ;                                                                                       Overflow
                      00012 ;       (65536 - <TMR1H::TMR1L>) =      ---------------------------------
                      00013 ;                                                                  prescaler * ciclo de máquina 
                      00014 ;
                      00015 ;                                                                               Overflow
                      00016 ;       <TMR1H::TMR1L> = 65536 - ---------------------------------
                      00017 ;                                                               prescaler * ciclo de máquina
                      00018 ;
                      00019 ;                                                          500us
                      00020 ;       <TMR1H::TMR1L> = 65536 - --------- = 65036
                      00021 ;                                                         1 * 1us
                      00022 ;
                      00023 ;       <TMR1H::TMR1L> = 65036d = FE0Ch
                      00024 ;
                      00025 ;       TMR1H = FEh
                      00026 ;       TMR1L = 0Ch
                      00027 ;
                      00028 ;       Tempo de latência obtido pelo debug = 14us
                      00029 ;
                      00030 ;                                                         (500us - 14us)
                      00031 ;       <TMR1H::TMR1L> = 65536 - ---------------- = 65050 (Valor corrigido considerando a latência de in
                            terrupção)
                      00032 ;                                                                 1 * 1us
                      00033 ;
                      00034 ;       <TMR1H::TMR1L> = 65050d = FE1Ah
                      00035 ;
                      00036 ;       TMR1H = FEh
                      00037 ;       TMR1L = 1Ah     
                      00038 ;
                      00039 ;               No debug e na simulação o programa funcionou como esperado, com o tempo de Overflow exat
                            o, com o tempo de 
                      00040 ;       latência corrigido.
                      00041 ;
