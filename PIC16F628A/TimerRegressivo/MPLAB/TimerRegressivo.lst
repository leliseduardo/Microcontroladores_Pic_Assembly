                      00001 ;
                      00002 ;               Este programa tem a função de criar um timer regressivo a partir do uso do display de 7 
                            segmentos. Para isso, 
                      00003 ;       utiliza-se o código da aula passada, denominada "Display7Segmentos", fazendo apenas algumas alte
                            rações.
                      00004 ;               O programa irá iniciar com a interrupão desligada e, quando um botão for pressionado, o 
                            display irá decrementar
                      00005 ;       e, quando chegar a zero, a interrupção será novamente desligada e um led irá se acender.
                      00006 ;               Este é um princípio de um temporiazador para alguma função prática. O led pode ser um re
                            lé que controla algum
                      00007 ;       atuador, que aciona por determinado tempo quando um botão é pressionado ou quando um sensor acio
                            na ou chega a 
                      00008 ;       determinado valor. 
                      00009 ;
                      00010 ;       *** Timer 1 ***
                      00011 ;       
                      00012 ;       Overflow = (65536 - <TMR1H::TMR1L>) * prescaler * ciclo de máquina
                      00013 ;               
                      00014 ;                                                                                       Overflow
                      00015 ;       (65536 - <TMR1H::TMR1L>) =      ---------------------------------
                      00016 ;                                                                  prescaler * ciclo de máquina 
                      00017 ;
                      00018 ;                                                                               Overflow
                      00019 ;       <TMR1H::TMR1L> = 65536 - ---------------------------------
                      00020 ;                                                               prescaler * ciclo de máquina
                      00021 ;
                      00022 ;                                                          200ms
                      00023 ;       <TMR1H::TMR1L> = 65536 - --------- = 15536
                      00024 ;                                                         4 * 1us
                      00025 ;
                      00026 ;       <TMR1H::TMR1L> = 15536d = 3CB0h
                      00027 ;
                      00028 ;       TMR1H = 3Ch
                      00029 ;       TMR1L = B0h             
