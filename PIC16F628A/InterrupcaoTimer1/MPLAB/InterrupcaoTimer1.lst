                      00002 ;               O objetivo deste programa é demonstrar como fazer a configuração da interrupção do timer
                            1. A simples função será
                      00003 ;       mudar o estado do pino RB7 a cada interrupção, mas é um bom jeito de iniciar na interrupção dest
                            e timer.
                      00004 ;
                      00005 ;       *** Timer 1 ***
                      00006 ;       
                      00007 ;       Overflow = (65536 - <TMR1H::TMR1L>) * prescaler * ciclo de máquina
                      00008 ;       Overflow = 65536 * 1 * 1us
                      00009 ;       Overflow = 65,536ms
                      00010 ;
                      00011 ;               Na simulação e na prática o programa funcionou perfeitamente.
                      00012 ;
